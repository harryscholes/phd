title = {{Analysis of canonical and non-canonical splice sites in mammalian genomes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=113136{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {28},
