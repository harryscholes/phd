journal = {DNA repair},
keywords = {Ataxia Telangiectasia Mutated Proteins,Cell Cycle Proteins,Cell Cycle Proteins: metabolism,DNA Breaks, Double-Stranded,DNA Repair,DNA Repair: genetics,DNA Repair: physiology,DNA-Binding Proteins,DNA-Binding Proteins: metabolism,G2 Phase,G2 Phase: physiology,Heterochromatin,Heterochromatin: metabolism,Intracellular Signaling Peptides and Proteins,Intracellular Signaling Peptides and Proteins: met,Nuclear Proteins,Nuclear Proteins: metabolism,Phosphorylation,Protein-Serine-Threonine Kinases,Protein-Serine-Threonine Kinases: metabolism,Repressor Proteins,Repressor Proteins: metabolism,Tumor Suppressor Proteins,Tumor Suppressor Proteins: metabolism},
