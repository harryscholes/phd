journal = {Molecular Microbiology},
keywords = {Bacterial Outer Membrane Proteins,Bacterial Outer Membrane Proteins: chemistry,Bacterial Outer Membrane Proteins: metabolism,Binding Sites,Carrier Proteins,Carrier Proteins: chemistry,Carrier Proteins: metabolism,Cell Surface,Cell Surface: chemistry,Cell Surface: metabolism,Colicins,Colicins: chemistry,Colicins: genetics,Colicins: metabolism,Crystallography,Models,Molecular,Protein Structure,Receptors,Secondary,Tertiary,X-Ray},
