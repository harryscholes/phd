issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Animals,Carbon Isotopes,Diabetes Mellitus, Experimental,Diabetes Mellitus, Experimental: chemically induce,Diabetes Mellitus, Experimental: enzymology,Fasting,Magnetic Resonance Spectroscopy,Magnetic Resonance Spectroscopy: methods,Male,Myocardium,Myocardium: enzymology,Pyruvate Dehydrogenase Complex,Pyruvate Dehydrogenase Complex: metabolism,Rats,Rats, Wistar,Streptozocin,Streptozocin: pharmacology,Time Factors},
