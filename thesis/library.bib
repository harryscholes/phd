@article{Saunders2019,
abstract = {Human genomics is undergoing a step change from being a predominantly research-driven activity to one driven through health care as many countries in Europe now have nascent precision medicine programmes. To maximize the value of the genomic data generated, these data will need to be shared between institutions and across countries. In recognition of this challenge, 21 European countries recently signed a declaration to transnationally share data on at least 1 million human genomes by 2022. In this Roadmap, we identify the challenges of data sharing across borders and demonstrate that European research infrastructures are well-positioned to support the rapid implementation of widespread genomic data access. In a recent declaration, 21 European countries committed to sharing data on at least 1 million human genomes by 2022. This Roadmap emphasizes the challenges of genomic data sharing across borders and how European research infrastructures can support the rapid implementation of transnational access to genomic data.},
author = {Saunders, Gary and Baudis, Michael and Becker, Regina and Beltran, Sergi and B{\'{e}}roud, Christophe and Birney, Ewan and Brooksbank, Cath and Brunak, S{\o}ren and {Van den Bulcke}, Marc and Drysdale, Rachel and Capella-Gutierrez, Salvador and Flicek, Paul and Florindi, Francesco and Goodhand, Peter and Gut, Ivo and Heringa, Jaap and Holub, Petr and Hooyberghs, Jef and Juty, Nick and Keane, Thomas M. and Korbel, Jan O. and Lappalainen, Ilkka and Leskosek, Brane and Matthijs, Gert and Mayrhofer, Michaela Th. and Metspalu, Andres and Navarro, Arcadi and Newhouse, Steven and Nyr{\"{o}}nen, Tommi and Page, Angela and Persson, Bengt and Palotie, Aarno and Parkinson, Helen and Rambla, Jordi and Salgado, David and Steinfelder, Erik and Swertz, Morris A. and Valencia, Alfonso and Varma, Susheel and Blomberg, Niklas and Scollen, Serena},
