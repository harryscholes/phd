issn = {1546-1718},
journal = {Nature genetics},
keywords = {Chromosomes, Human,Chromosomes, Human: genetics,DNA,DNA: genetics,Gene Dosage,Gene Dosage: genetics,Genetic Variation,Genome, Human,Haplotypes,Haplotypes: genetics,Humans,Oligonucleotide Array Sequence Analysis,Polymerase Chain Reaction,Polymorphism, Single Nucleotide,Population Groups,Population Groups: genetics},
