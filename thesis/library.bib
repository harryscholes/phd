title = {{Fundamentals of ion mobility spectrometry}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S1367593117301229},
volume = {42},
year = {2018}
}
@article{Hurst2009,
abstract = {The Single Amino Acid Polymorphism database(SAAPdb) is a new resource for the analysis and visualization of the structural effects of mutations. Our analytical approach is to map single nucleotide polymorphisms(SNPs) and pathogenic deviations(PDs) to protein structural data held within the Protein Data Bank. By mapping mutations onto protein structures, we can hypothesize whether the mutant residues will have any local structural effect that may "explain" a deleterious phenotype. Our prior work used a similar approach to analyze mutations within a single protein. An analysis of the contents of SAAPdb indicates that there are clear differences in the sequence and structural characteristics of SNPs and PDs, and that PDs are more often explained by our structural analysis. This mapping and analysis is a useful resource for the mutation community and is publicly available at http://www.bioinf.org.uk/saap/db/. {\textcopyright} 2009 Wiley-Liss, Inc.},
author = {Hurst, Jacob M. and McMillan, Lisa E.M. and Porter, Craig T. and Allen, James and Fakorede, Adebola and Martin, Andrew C.R.},
