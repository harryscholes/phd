title = {{Assessment of computational methods for predicting the effects of missense mutations in human cancers.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3665581{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {14 Suppl 3},
year = {2013}
}
