journal = {RNA (New York, N.Y.)},
keywords = {Alternative Splicing,Epigenesis, Genetic,Exons,Humans,Jurkat Cells,Nucleosomes,Nucleosomes: metabolism,RNA, Messenger,RNA, Messenger: genetics,RNA, Messenger: metabolism,Ribonucleoproteins,Ribonucleoproteins: physiology,Transcriptome},
month = {dec},
number = {12},
pages = {2053--66},
pmid = {26437669},
title = {{Global analysis of physical and functional RNA targets of hnRNP L reveals distinct sequence and epigenetic features of repressed and enhanced exons.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26437669},
volume = {21},
year = {2015}
