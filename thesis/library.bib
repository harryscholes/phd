journal = {Nature},
keywords = {2014 redacted exam paper,Bacterial Proteins,Bacterial Proteins: chemistry,Bacterial Proteins: metabolism,Bile Acids and Salts,Bile Acids and Salts: metabolism,Biological Transport,Carrier Proteins,Carrier Proteins: chemistry,Carrier Proteins: metabolism,Cell Membrane,Cell Membrane: metabolism,Crystallography,Membrane Glycoproteins,Membrane Glycoproteins: chemistry,Membrane Glycoproteins: metabolism,Models,Molecular,Protein Conformation,Reproducibility of Results,Rotation,Sodium,Sodium: metabolism,Structure-Activity Relationship,X-Ray,Yersinia,Yersinia: chemistry},
mendeley-tags = {2014 redacted exam paper},
