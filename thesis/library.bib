journal = {Biochimica et biophysica acta},
keywords = {Eukaryota,Eukaryota: genetics,Eukaryotic Cells,Eukaryotic Cells: metabolism,Gene Expression Regulation,Humans,Promoter Regions, Genetic,RNA Polymerase II,Transcription Factors,Transcription Factors: genetics,Transcription Factors: metabolism,Transcription, Genetic},
