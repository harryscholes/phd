issn = {1362-4962},
journal = {Nucleic acids research},
keywords = {Computational Biology,Databases, Nucleic Acid,Enhancer Elements, Genetic,Humans,Internet,Mutation,RNA Splice Sites,Sequence Analysis, RNA,Silencer Elements, Transcriptional,Software,User-Computer Interface},
month = {may},
number = {9},
