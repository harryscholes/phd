journal = {Biophysical journal},
keywords = {Antibodies,Antibodies: immunology,Bacterial Outer Membrane Proteins,Bacterial Outer Membrane Proteins: metabolism,Cell Membrane,Cell Membrane: metabolism,Colicins,Colicins: metabolism,Computer Simulation,Diffusion,Escherichia coli,Escherichia coli Proteins,Escherichia coli Proteins: metabolism,Escherichia coli: metabolism,Fluorescent Dyes,Fluorescent Dyes: metabolism,Membrane Proteins,Membrane Proteins: metabolism,Membrane Transport Proteins,Membrane Transport Proteins: metabolism,Microscopy, Fluorescence,Models, Biological,Monte Carlo Method,Multiprotein Complexes,Multiprotein Complexes: metabolism,Porins,Porins: metabolism,Protein Binding,Protein Transport,Surface Properties},
