pages = {105--113},
pmid = {23528454},
publisher = {Elsevier},
title = {{Mass Spectrometry-based Quantitative Proteomic Profiling of Human Pancreatic and Hepatic Stellate Cell Lines}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23528454 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4123426 http://linkinghub.elsevier.com/retrieve/pii/S1672022913000326},
