issn = {03051048},
journal = {Nucleic Acids Research},
keywords = {amino acids,benchmarking,phylogeny,reconstructive surgical procedures,uniprot knowledgebase},
month = {jul},
number = {12},
pages = {e121--e121},
pmid = {23598997},
publisher = {Oxford University Press},
title = {{Challenges in homology search: HMMER3 and convergent evolution of coiled-coil regions}},
url = {https://academic.oup.com/nar/article/41/12/e121/1025950},
volume = {41},
year = {2013}
}
@article{Misonou2000,
