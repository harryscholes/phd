isbn = {1476-4687 (Electronic)$\backslash$r0028-0836 (Linking)},
issn = {0028-0836},
journal = {Nature},
number = {7349},
pages = {49--53},
pmid = {21637253},
title = {{Crystal structure of the FimD usher bound to its cognate FimC-FimH substrate.}},
volume = {474},
year = {2011}
}
