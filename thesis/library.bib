doi = {10.1371/journal.pone.0031813},
file = {:Users/harry/Dropbox/Mendeley/Rojas et al. - 2012 - Uncovering the molecular machinery of the human spindle-an integration of wet and dry systems biology.pdf:pdf},
isbn = {2003505520},
