file = {:Users/harry/Dropbox/Mendeley/Petersen et al. - 1983 - Partial primary structure of bovine plasma fibronectin three types of internal homology.pdf:pdf},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Amino Acid Sequence,Animals,Binding Sites,Cattle,Collagen,Collagen: metabolism,Disulfides,Fibrinolysin,Fibronectins,Heparin,Heparin: metabolism,Molecular Weight,Peptide Fragments,Protein Conformation},
month = {jan},
