pages = {281--94},
pmid = {24651194},
publisher = {Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
shorttitle = {Nat Chem},
title = {{The power of ion mobility-mass spectrometry for structural characterization and the study of conformational dynamics.}},
