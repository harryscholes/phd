journal = {Acta Crystallographica Section D Biological Crystallography},
month = {oct},
number = {10},
pages = {1341--1349},
publisher = {International Union of Crystallography},
title = {{Fitting atomic models into electron-microscopy maps}},
url = {http://scripts.iucr.org/cgi-bin/paper?S0907444900009562},
volume = {56},
