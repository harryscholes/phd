keywords = {ALDH2,HNE,Memory loss,Mitochondria,Oxidative stress,Transgenic mice},
title = {{Age-dependent neurodegeneration accompanying memory loss in transgenic mice defective in mitochondrial aldehyde dehydrogenase 2 activity}},
year = {2008}
}
