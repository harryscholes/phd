journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Mitochondria,Neurodegeneration,Oxidative phosphorylation},
title = {{Cytochrome c oxidase deficiency in neurons decreases both oxidative stress and amyloid formation in a mouse model of Alzheimer's disease}},
year = {2007}
}
