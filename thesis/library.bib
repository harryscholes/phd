journal = {Nature protocols},
keywords = {Detergents,Detergents: chemistry,Indicators and Reagents,Indicators and Reagents: chemistry,Mass Spectrometry,Mass Spectrometry: instrumentation,Mass Spectrometry: methods,Membrane Proteins,Membrane Proteins: chemistry,Membrane Proteins: isolation {\&} purification,Phospholipids,Phospholipids: chemistry,Phospholipids: metabolism,Protein Binding,Protein Structure, Tertiary},
