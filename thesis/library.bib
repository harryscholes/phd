issn = {1545-7885},
journal = {PLoS biology},
keywords = {Bacterial Proteins,Bacterial Proteins: chemistry,Crystallography, X-Ray,Protein Binding,Protein Kinases,Protein Kinases: chemistry,Protein Structure, Secondary,Protein Structure, Tertiary,Streptococcus mutans,Streptococcus mutans: metabolism},
month = {jan},
