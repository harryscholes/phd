issn = {1752-0509},
journal = {BMC Systems Biology},
number = {1},
pages = {92},
pmid = {22846459},
title = {{HINT: High-quality protein interactomes and their applications in understanding human disease}},
url = {http://bmcsystbiol.biomedcentral.com/articles/10.1186/1752-0509-6-92},
volume = {6},
year = {2012}
}
