journal = {Human genomics},
keywords = {Amino Acid Substitution,Amino Acid Substitution: genetics,Computational Biology,Humans,Internet,Markov Chains,Mutation,Mutation: genetics,Phenotype,Polymorphism, Single Nucleotide,Polymorphism, Single Nucleotide: genetics,Software},
number = {1},
pages = {11},
pmid = {24980617},
title = {{Ranking non-synonymous single nucleotide polymorphisms based on disease concepts.}},
