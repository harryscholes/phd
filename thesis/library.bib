journal = {Nature},
keywords = {Acetylation,Acyl-CoA Dehydrogenase,Adenosine Triphosphate/biosynthesis/metabolism,Adipose Tissue,Animals,Body Temperature Regulation,Brown/enzymology/metabolism,Caloric Restriction,Carnitine/analogs {\&} derivatives/metabolism,Cell Line,Cold Temperature,Fasting/metabolism,Fatty Acids/*metabolism,Humans,Hypoglycemia/metabolism,Liver/enzymology/metabolism,Long-Chain/chemistry/*meta,Male,Mass Spectrometry,Mice,Mitochondria/*enzymology/*metabolism,Oxidation-Reduction,Sirtuin 3/deficiency/genetics/*metabolism,Triglycerides/metabolism,Up-Regulation},
