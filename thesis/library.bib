doi = {10.1073/pnas.1013643108},
file = {:Users/harry/Dropbox/Mendeley/Derebe et al. - 2011 - Structural studies of ion permeation and Ca2 blockage of a bacterial channel mimicking the cyclic nucleotide-gate.pdf:pdf},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Amino Acid Sequence,Animals,Bacillus cereus,Bacillus cereus: metabolism,Bacterial Proteins,Bacterial Proteins: chemistry,Calcium,Calcium: chemistry,Cattle,Humans,Ion Channel Gating,Ions,Ions: chemistry,Ligands,Molecular Sequence Data,Mutagenesis,Potassium,Potassium: chemistry,Protein Conformation,Recombinant Proteins,Recombinant Proteins: chemistry,Sequence Homology, Amino Acid,Sodium,Sodium: chemistry},
month = {jan},
