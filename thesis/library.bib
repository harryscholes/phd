title = {{Quantitative phenotyping via deep barcode sequencing}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19622793 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2765281},
volume = {19},
year = {2009}
}
