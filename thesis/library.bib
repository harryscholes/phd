@article{Suttie2012,
abstract = {We report the first comparison of cardiovascular magnetic resonance imaging (CMR) at 1.5 T, 3 T and 7 T field strengths using steady state free precession (SSFP) and fast low angle shot (FLASH) cine sequences. Cardiac volumes and mass measurements were assessed for feasibility, reproducibility and validity at each given field strength using FLASH and SSFP sequences. Ten healthy volunteers underwent retrospectively electrocardiogram (ECG) gated CMR at 1.5 T, 3 T and 7 T using FLASH and SSFP sequences. B1 and B0 shimming and frequency scouts were used to optimise image quality. Cardiac volume and mass measurements were not significantly affected by field strength when using the same imaging sequence (P {\textgreater} 0.05 for all parameters at 1.5 T, 3 T and 7 T). SSFP imaging returned larger end diastolic and end systolic volumes and smaller left ventricular masses than FLASH imaging at 7 T, and at the lower field strengths (P {\textless} 0.05 for each parameter). However, univariate general linear model analysis with fixed effects for sequence and field strengths found an interaction between imaging sequence and field strength (P = 0.03), with a smaller difference in volumes and mass measurements between SSFP and FLASH imaging at 7 T than 1.5 T and 3 T. SSFP and FLASH cine imaging at 7 T is technically feasible and provides valid assessment of cardiac volumes and mass compared with CMR imaging at 1.5 T and 3 T field strengths. Copyright {\textcopyright} 2011 John Wiley {\&} Sons, Ltd.},
author = {Suttie, J J and Delabarre, L and Pitcher, a and van de Moortele, P F and Dass, S and Snyder, C J and Francis, J M and Metzger, G J and Weale, P and Ugurbil, K and Neubauer, S and Robson, M and Vaughan, T},
doi = {10.1002/nbm.1708},
file = {:Users/harry/Dropbox/Mendeley/Suttie et al. - 2012 - 7 Tesla (T) human cardiovascular magnetic resonance imaging using FLASH and SSFP to assess cardiac function valid.pdf:pdf},
issn = {1099-1492},
journal = {NMR in biomedicine},
keywords = {7 tesla,cardiac function,cardiovascular magnetic resonance imaging,clinical validation,high field strength},
month = {jan},
number = {1},
pages = {27--34},
pmid = {21774009},
title = {{7 Tesla (T) human cardiovascular magnetic resonance imaging using FLASH and SSFP to assess cardiac function: validation against 1.5 T and 3 T.}},
