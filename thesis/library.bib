title = {{Chk1 is a wee1 kinase in the G2 DNA damage checkpoint inhibiting cdc2 by Y15 phosphorylation.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1169658{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {16},
