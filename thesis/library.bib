@article{Mullan1992,
abstract = {Mutations at codon 717 in exon 17 of the $\beta$–amyloid precursor protein (APP) gene have previously been shown to segregate with early onset Alzheimer's disease in some families. We have identified a double mutation at codons 670 and 671 (APP 770 transcript) in exon 16 which co–segregates with the disease in two large (probably related) early–onset Alzheimer's disease families from Sweden. Two base pair transversions (G to T, A to C) from the normal sequence predict Lys to Asn and Met to Leu amino acid substitutions at codons 670 and 671 of the APP transcript. This mutation occurs at the amino terminal of $\beta$–amyloid and may be pathogenic because it occurs at or close to the endosomal/lysosomal cleavage site of the molecule. Thus, pathogenic mutations in APP frame the $\beta$–amyloid sequence. {\textcopyright} 1992 Nature Publishing Group.},
