title = {{Dictionary of protein secondary structure: Pattern recognition of hydrogen-bonded and geometrical features}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/6667333 http://onlinelibrary.wiley.com/doi/10.1002/bip.360221211/abstract{\%}5Cnhttp://onlinelibrary.wiley.com/doi/10.1002/bip.360221211/full{\%}5Cnhttp://onlinelibrary.wiley.com/store/10.1002/bip.360221211/asset/360221211{\_}ftp.pdf},
