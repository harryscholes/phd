keywords = {Amino Acid Sequence,Animals,Aquaporin 1,Aquaporins,Aquaporins: chemistry,Aquaporins: metabolism,Biological Transport,Blood Group Antigens,Crystallography, X-Ray,Humans,Ion Transport,Models, Molecular,Molecular Sequence Data,Protein Conformation,Sequence Homology, Amino Acid,Structure-Activity Relationship,Water,Water: metabolism},
