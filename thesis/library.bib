month = {jun},
number = {7508},
pages = {191--197},
pmid = {25008524},
title = {{NMDA receptor structures reveal subunit arrangement and pore architecture}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4263351{\&}tool=pmcentrez{\&}rendertype=abstract},
