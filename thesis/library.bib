keywords = {Binding Sites,Colicins,Colicins: chemistry,Colicins: metabolism,Escherichia coli,Escherichia coli Proteins,Escherichia coli Proteins: chemistry,Escherichia coli Proteins: metabolism,Escherichia coli: genetics,Escherichia coli: metabolism,Fluorescence Resonance Energy Transfer,Kinetics,Models, Molecular,Periplasmic Proteins,Periplasmic Proteins: chemistry,Periplasmic Proteins: metabolism,Protein Conformation,Protein Folding,Protein Structure, Tertiary,Protein Transport},
month = {may},
