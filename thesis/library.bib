month = {jul},
number = {14},
pages = {6498--502},
pmid = {8341661},
title = {{A general two-metal-ion mechanism for catalytic RNA.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=46959{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {90},
