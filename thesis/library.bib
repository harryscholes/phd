issn = {0022-2836},
journal = {Journal of molecular biology},
keywords = {Animals,DNA,Exons,Genes,Genome, Human,Humans,Introns,Markov Chains,Models, Genetic,Molecular Sequence Data,Probability,Promoter Regions, Genetic,Proteins,Proteins: genetics,RNA Splicing,Sequence Analysis, DNA,Software,Vertebrates,Vertebrates: genetics},
month = {apr},
