journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Allosteric Site,Animals,Binding Sites,Cations,Crystallography, X-Ray,Cyanobacteria,Cyanobacteria: metabolism,Escherichia coli,Escherichia coli: metabolism,Hydrogen-Ion Concentration,Ion Channels,Ion Channels: chemistry,Ligand-Gated Ion Channels,Ligand-Gated Ion Channels: chemistry,Ligands,Models, Molecular,Oocytes,Oocytes: metabolism,Protein Structure, Quaternary,Protein Structure, Tertiary,Signal Transduction,Xenopus laevis},
